<?xml version="1.0"?>
<sdf version="1.4">
  <world name="default">
  
    <!-- A global light source -->
    <include>
      <uri>model://sun</uri>
    </include>

    <!-- A ground plane -->
    <include>
      <uri>model://ground_plane</uri>
    </include>

    <!-- A camera, focused down at the origin -->
    <gui>
      <camera name='user_camera'>
        <pose>0 0 5 -1.5708 1.5708 0</pose>
      </camera>
    </gui>

  </world>
</sdf>